msgctxt "#32600"
msgid "History"
msgstr "Geschiedenis"

msgctxt "#32601"
msgid "[B]See which files have been cleaned recently.[/B]"
msgstr "[B]Bekijk welke bestanden onlangs opgeschoond zijn.[/B]"

msgctxt "#32602"
msgid "View cleaning history"
msgstr "Bekijk schoonmaakgeschiedenis"

msgctxt "#32603"
msgid "[B]Cleaning Log[/B]"
msgstr "[B]Schoonmaaklogboek[/B]"

